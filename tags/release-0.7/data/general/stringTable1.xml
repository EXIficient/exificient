<?xml version="1.0" encoding="UTF-8"?>
<root xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="stringTable1.xsd">
    <!--
        # Compression
        [Structure]
        [ # b #
            abc (miss)
            foo (miss)
            X   (miss) 
            xyz (miss)
        ]
        [ # a #
            X   (global-hit to a)
        ]
    -->
    <b>abc</b>
    <a>X</a>
    <b>foo</b>
    <b>X</b>
    <b>xyz</b>
</root>
