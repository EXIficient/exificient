<?xml version="1.0" encoding="UTF-8"?>
<header xmlns="http://www.w3.org/2009/exi" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.w3.org/2009/exi ./EXIOptionsHeader.xsd">
    <lesscommon>
        <uncommon>
            <alignment>
                <pre-compress/>
            </alignment>
            <selfContained/>
